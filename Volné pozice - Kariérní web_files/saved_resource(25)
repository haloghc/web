    var captcha = reduxFormValidators.addValidator({
      validator: function validator(options, value, allValues, props) {
        var formData = new FormData();
        formData.append('captchaText', value);
        var xhr = new XMLHttpRequest();
        xhr.open('POST', props.appOptions.captchaCheckURL, false);
        xhr.send(formData);
        if (xhr.responseText == 'false') {
          return {
            id: "form.errors.captcha",
            defaultMessage: "form.errors.captcha",
            values: {}
          };
        } else {
          return undefined;
        }
      }
    });

