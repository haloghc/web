    var submitFormAndRedirectToResultPage = function submitFormAndRedirectToResultPage(formData, history, url, formName, captchaFieldName, captchaServeURL) {
      return function (dispatch, getState) {
        if (isSubmitting(getState())) {
          return;
        }
        dispatch(captchaProtectDoubleSubmitAction(true));
        fetch(url, {
          method: 'POST',
          cache: "no-cache",
          body: formData
        }).then(function (response) {
          if (response && response.ok) {
            history.push('/success');
          } else {
            if (response.status == 400) {
              return response.json().then(function (data) {
                if (data) {
                  if (data.header.error.errorCode == 'CAPTCHA_INVALID') {
                    dispatch(captchaProtectDoubleSubmitAction(false));
                    dispatch(captchaInvalidAction(captchaServeURL, formName, captchaFieldName));
