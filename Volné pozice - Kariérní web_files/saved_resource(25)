      return (hint === "string" ? String : Number)(input);
    }
    function _toPropertyKey(arg) {
      var key = _toPrimitive(arg, "string");
      return typeof key === "symbol" ? key : String(key);
    }

    var SIZE_REG = /^([\d.]+)\s*([KMGTPE]?B)?$/;
    var SIZE_UNITS = {
      B: 1,
      KB: 1024,
      MB: 1048576,
      GB: 1073741824,
      TB: 1099511627776,
      PB: 1125899906842624,
      EB: 1152921504606847000
    };
    var parse = function parse(size) {
      return SIZE_REG.exec(('' + size).trim());
    };
    var sizeToInt = function sizeToInt(size) {
      var pair = parse(size);
      if (pair) return pair[1] * (SIZE_UNITS[pair[2]] || 1);
      /* istanbul ignore else */
      if (typeof console !== 'undefined') {
        console.error("file validator: size \"".concat(size, "\" unknown"));
      }
      return null;
    };
    var isFileList$2 = function isFileList(value) {
      if (typeof FileList !== 'undefined' && value instanceof FileList || typeof File !== 'undefined' && (value instanceof File || value[0] instanceof File)) {
        return true;
      }
      return false;
    };
    var removeWithConfirm = function removeWithConfirm(name, value, inputElement, fieldActions, intl, removeAction) {
      var confirmResult = window.confirm(intl.formatMessage({
        id: "components.fileInputWithEditField.confirmRemoveAction"
      }));
      if (confirmResult === true) {
        inputElement.value = null;
        fieldActions.change(name, null);
        fieldActions.touch(name);
        removeAction && removeAction({
          inputName: name,
          inputValue: value
        });
      }
    };
