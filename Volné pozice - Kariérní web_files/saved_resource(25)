    var _onChange = function onChange(e, regionsBasedOnCode, onChangeAction) {
      var regionIds = (e || []).map(function (region) {
        return regionsBasedOnCode[region.id].id;
      });
      onChangeAction(regionIds);
    };
    var handleLocationMouseOver = function handleLocationMouseOver(e, regionsBasedOnCode, onUpdateTooltipAction) {
      var target = e.target;
      var regionLabel = regionsBasedOnCode[target.id].label;
      var mapToolTip = document.getElementsByClassName("jobs-rsd-map-tooltip")[0];
      if (mapToolTip) {
        mapToolTip.classList.add('active');
        mapToolTip.innerHTML = regionLabel;
      }

      //onUpdateTooltipAction(regionLabel);
    };

    var handleLocationMouseOut = function handleLocationMouseOut(onUpdateTooltipAction) {
      var mapToolTip = document.getElementsByClassName("jobs-rsd-map-tooltip")[0];
      if (mapToolTip) {
        mapToolTip.classList.remove('active');
      }

      ///onUpdateTooltipAction("");
    };

    var handleLocationMouseMove = function handleLocationMouseMove(e) {
      var mapToolTip = document.getElementsByClassName("jobs-rsd-map-tooltip")[0];
      if (mapToolTip) {
        mapToolTip.style.left = e.clientX + 'px';
        mapToolTip.style.top = e.clientY - 40 + 'px';
      }
    };
