    var overviewReducer = function overviewReducer() {
      var state = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : {
        mapResetSwitchState: true
      };
      var action = arguments.length > 1 ? arguments[1] : undefined;
      switch (action.type) {
        case actionTypes.CLEAR_FILTER:
          return ramda.mergeDeepRight(state, {
            selectedCareerFieldIds: [],
            selectedRegionIds: [],
            jobTitle: "",
            jobTitleValue: "",
            mapResetSwitchState: !state.mapResetSwitchState
          });
        case actionTypes.UPDATE_CAREER_FIELD_ID:
          return ramda.mergeDeepRight(state, {
            selectedCareerFieldIds: action.payload
          });
        case actionTypes.UPDATE_REGION_IDS:
          return ramda.mergeDeepRight(state, {
            selectedRegionIds: action.payload
          });
        case actionTypes.UPDATE_JOB_TITLE:
          return ramda.mergeDeepRight(state, {
            jobTitle: action.payload
          });
        case actionTypes.UPDATE_JOB_TITLE_VALUE:
          return ramda.mergeDeepRight(state, {
            jobTitleValue: action.payload
          });
        case actionTypes.APPEND_ADVERTISEMENTS:
          return ramda.mergeDeepRight(state, {
            advertisements: appendAdvertisements(state, action.payload)
          });
        case actionTypes.REPLACE_ADVERTISEMENTS:
          return ramda.mergeDeepRight(clearCountsByByCategories(state), {
            advertisements: action.payload
          });
        case actionTypes.UPDATE_MAP_TOOLTIP:
          return ramda.mergeDeepRight(state, {
            mapTooltipText: action.payload
          });
        default:
          return state;
      }
    };
    var clearCountsByByCategories = function clearCountsByByCategories(state) {
      var advertisements = state.advertisements || {};
      advertisements.countsByByCategories = {};
      return _objectSpread2(_objectSpread2({}, state), {}, {
        advertisements: advertisements
      });
    };
    var appendAdvertisements = function appendAdvertisements(state, incomingAdvertisements) {
      var _advertisements$items;
      var advertisements = state.advertisements;
      (_advertisements$items = advertisements.items).push.apply(_advertisements$items, _toConsumableArray(incomingAdvertisements.items));
      advertisements.totalItemsCount = incomingAdvertisements.totalItemsCount;
      advertisements.page = incomingAdvertisements.page;
      advertisements.countsByByCategories = incomingAdvertisements.countsByByCategories;
      return advertisements;
    };

    var localizationReducer = function localizationReducer() {
      var state = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : {};
      var action = arguments.length > 1 ? arguments[1] : undefined;
      switch (action.type) {
        case localizationActionTypes.FORM_DATA_REPLACE:
          return action.payload;
        default:
          return state;
      }
    };

    var rootReducer = redux.combineReducers({
      overview: overviewReducer,
