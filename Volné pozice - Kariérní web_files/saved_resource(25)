            ignoreAccents = _ignoreCase$ignoreAcc.ignoreAccents,
            stringify = _ignoreCase$ignoreAcc.stringify,
            trim = _ignoreCase$ignoreAcc.trim,
            matchFrom = _ignoreCase$ignoreAcc.matchFrom;

