        _this.handleEventDelta = function (event, delta) {
          var _this$props = _this.props,
              onBottomArrive = _this$props.onBottomArrive,
              onBottomLeave = _this$props.onBottomLeave,
              onTopArrive = _this$props.onTopArrive,
