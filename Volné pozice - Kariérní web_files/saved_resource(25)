      if (careerFieldIds && careerFieldIds.length > 0) {
        careerFieldIds.filter(function (c) {
          return c != null;
        }).forEach(function (careerFieldId) {
          requestBody.append("filter.careerFieldIds", careerFieldId);
        });
      }
      var jobTitle = getJobTitle(state);
      if (hasJobTitleValidLength(state)) {
        requestBody.append("filter.jobTitle", jobTitle);
      }
      return requestBody;
    };

