    var convertRegionsBasedOnCode = function convertRegionsBasedOnCode(regions) {
      return regions.reduce(function (obj, item) {
        obj[item.code] = item;
        return obj;
      }, {});
    };
    var init = function init(portletElementId, configuration, initData) {
      configuration.portletInstance = configuration.portletInstance || {};
