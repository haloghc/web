    };
    var extractAndPassParams = function extractAndPassParams() {
      var newSearchParams = new URLSearchParams();
      var searchParams = new URLSearchParams(new URL(window.location.href).search || "");
      ["obor", "kraj", "pozice"].filter(function (param) {
        return searchParams.has(param);
      }).forEach(function (param) {
        return newSearchParams.append(param, searchParams.get(param));
      });
      return newSearchParams.size > 0 ? "?".concat(newSearchParams.toString()) : "";
    };
    function main(_ref) {
      _ref.portletNamespace;
      _ref.contextPath;
      var portletElementId = _ref.portletElementId,
          configuration = _ref.configuration;
      fetch("/o/career/advertisement/job/public/overview".concat(extractAndPassParams()), {
        method: 'GET',
        cache: "no-cache"
      }).then(function (response) {
        return response.json();
      }).then(function (initData) {
        init(portletElementId, configuration, initData);
      }).catch(function (error) {
        console.error(error);
      });
    }

    exports["default"] = main;
